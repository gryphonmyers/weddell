{
  "name": "weddell",
  "version": "3.5.10",
  "description": "A tiny, modular, nonopinionated front-end web framework.",
  "main": "dist/weddell.js",
  "module": "dist/weddell.mjs",
  "scripts": {
    "start": "budo docs/app.js:static/bundle.js --dir docs",
    "test": "ava",
    "docs": "jsdoc -c jsdoc.json",
    "check-bundle-size": "rollup -c rollup.config.esm.js | terser --config-file terser.config.json | gzip-size",
    "bundle": "rollup -c rollup.config.js -o dist/weddell.js",
    "bundle-esm": "rollup -c rollup.config.esm.js -o dist/weddell.mjs",
    "bundle-min": "rollup -c rollup.config.esm.js | terser --config-file terser.config.json -o dist/weddell.min.mjs"
  },
  "files": [
    "src",
    "lib",
    "dist"
  ],
  "author": "Gryphon Myers",
  "license": "MIT",
  "dependencies": {
    "@weddell/event-emitter-mixin": "^1.1.5",
    "@weddell/mixwith": "^0.1.5",
    "@weddell/vdom-virtualize": "^2.0.1",
    "array-compact": "^1.0.0",
    "array-uniq": "^1.0.3",
    "array.prototype.flat": "^1.2.1",
    "ava": "^3.6.0",
    "deep-equal": "^1.0.1",
    "defaults-es6": "^1.1.1",
    "document-ready-promise": "^3.0.1",
    "fast-deep-equal": "^2.0.1",
    "find-parent": "^2.0.2",
    "isomitter": "^0.1.0",
    "nanomorph": "^5.4.2",
    "path-to-regexp": "^1.7.0",
    "vdom-to-html": "^2.3.1",
    "virtual-dom": "^2.1.1"
  },
  "devDependencies": {
    "better-docs": "^2.3.0",
    "browserify": "^16.2.3",
    "budo": "^11.2.2",
    "colors": "^1.1.2",
    "del": "^3.0.0",
    "disc": "^1.3.2",
    "es6-promisify": "^5.0.0",
    "esm": "^3.2.25",
    "gulp": "^3.9.1",
    "gulp-if": "^2.0.2",
    "gulp-uglify": "^3.0.0",
    "gzip-size-cli": "^3.0.0",
    "jsdoc": "^3.6.5",
    "jsdoc-to-markdown": "^5.0.0",
    "jsdom": "^15.1.1",
    "lodash": "^4.17.4",
    "mz": "^2.6.0",
    "prettysize": "^0.1.0",
    "rollup": "^1.21.4",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "stream-to-promise": "^2.2.0",
    "terser": "^4.3.4",
    "typescript": "^4.0.3",
    "uglify-es": "^3.0.24",
    "uglifyify": "^3.0.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.9.0"
  }
}
